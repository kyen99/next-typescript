{
  "parser": "typescript-eslint-parser",
  "parserOptions": {
    "jsx": true
  },
  "extends": [
    "airbnb",
    "typescript"
  ],
  "plugins": [
    "typescript"
  ],
  "rules": {
    "semi": ["error", "never"],
    "no-console": "warn",
    "react/jsx-filename-extension": ["warn", { "extensions": [".ts", ".tsx", "js", "jsx"] }],
    "typescript/member-delimiter-style": "off"
  }
}
