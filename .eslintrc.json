{
  "parser": "typescript-eslint-parser",
  "parserOptions": {
    "jsx": true
  },
  "extends": [
    "airbnb",
    "typescript"
  ],
  "plugins": [
    "typescript"
  ],
  "rules": {
    "semi": ["error", "never"],
    "react/jsx-filename-extension": [1, { "extensions": [".ts", ".tsx", "js", "jsx"] }]
  }
}
